{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "alwaysStrict": true,
    "pretty": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@structures": [
        "./src/structures"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@controllers": [
        "./src/controllers"
      ],
      "@database": [
        "./src/database"
      ],
      "@database/*": [
        "./src/database/*"
      ],
      "@Controller": [
        "./src/controllers/Controller"
      ],
      "@errors": [
        "./src/errors"
      ],
      "@middlewares": [
        "./src/middlewares"
      ],
      "@getaway": [
        "./src/getaway"
      ],
      "@config": [
        "./src/config"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
